<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="230.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.171"
>
    <!--fx:controller="Controller.ExportSceneController">-->
    <stylesheets>
        <URL value="@CSS/defaultTheme.css"/>
    </stylesheets>
    <children>
        <VBox alignment="CENTER" layoutX="-1.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="232.0"
              prefWidth="260.0" spacing="10.0">
            <children>
                <Label text="Select an output"/>
                <Label fx:id="outputLabel" text=""/>
                <JFXButton fx:id="selectButton" buttonType="RAISED" text="Select Output"/>
                <JFXButton fx:id="exportButton" buttonType="RAISED" disable="true" text="Export"/>
                <JFXButton fx:id="cancelButton" buttonType="RAISED" styleClass="cancelButton" text="Cancel"/>
            </children>
        </VBox>
    </children>

</StackPane>
